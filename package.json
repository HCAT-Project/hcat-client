    "pinia": "^2.0.35",
    "pinia-plugin-persistedstate": "^3.1.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.5",
    "@iconify-json/carbon": "^1.1.16",
    "@types/node": "^18.15.12",
    "@unocss/reset": "^0.51.5",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue-macros/volar": "^0.9.5",
    "@vue/test-utils": "^2.3.2",
    "eslint": "^8.38.0",
    "jsdom": "^21.1.1",
